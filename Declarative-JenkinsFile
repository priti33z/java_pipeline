pipeline {
    agent any
    
    tools{
        maven "Maven-3.9.11"
    }

    stages {
        stage('Clone') {
            steps {
              https://github.com/priti33z/java_pipeline.git
            }
        }
         stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
